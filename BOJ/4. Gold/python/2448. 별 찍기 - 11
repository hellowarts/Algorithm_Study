graph = [['*', '*', '*', '*', '*'],
         [' ', '*', '*', '*', ' '],
         [' ', ' ', '*', ' ', ' ']]

n = int(input())

def star(N, before):
    after = [[' ']*(2*(N+1)-1) for _ in range(2*(N+1)+1)]
    for i in range(2*N+1):
        after[i][N:N+N*2] = before[i]
    k=0
    for i in range(N,2*N):
        after[i][:N*2] = before[k]
        after[i][2*N:2*N+len(before[k])] = before[k]
        k+=1
    if N+1 == n:
        return after
    return star(N+1, after)
if n == 1:
    result = ['*']
elif n == 2:
    result = graph
else:
    result = star(2, graph)

for i in result:
    print("".join(i))
