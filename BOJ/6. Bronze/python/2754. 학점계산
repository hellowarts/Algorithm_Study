import sys

Sum = 0
grad = sys.stdin.readline().strip()

if grad[0] == 'A':
    Sum += 4.0
elif grad[0] == 'B':
    Sum += 3.0
elif grad[0] == 'C':
    Sum += 2.0
elif grad[0] == 'D':
    Sum += 1.0
else:
    Sum += 0.0

if grad[0] == 'F':
    pass
elif grad[1] == '+':
    Sum += 0.3
elif grad[1] == '-':
    Sum -= 0.3

print(Sum)
